<!DOCTYPE html>
<html lang="en">
    <%- include('./partitals/head.ejs')  %>
<body>

    <div class="container">
        <div class="content">
            <% if (users.length > 0) { %> <% users.forEach(user => { %>

                <h3>
                    <%= user.firstName %>
                    <%= user.lastName %>
                </h3>
          
                <p>
                    <%= user.email %>
                </p>
          
                <% }) %> <% } else { %>
                    <p>Não há usuários</p>
                <% } %>
        </div>
    </div>

</body>
</html>